  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.10.14",
    "@mui/styled-engine-sc": "^5.10.14",
    "axios": "^1.2.0",
    "i18next": "^22.4.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.6",
    "styled-components": "^5.3.6",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "serve": "env-cmd -f ./.env-local react-app-rewired start",
    "serve:local": "env-cmd -f ./.env-local react-app-rewired start",
    "serve:dev": "env-cmd -f ./.env.dev react-app-rewired start",
    "serve:prod": "env-cmd -f ./.env.prod react-app-rewired start",
    "serve:storybook": "start-storybook -p 6006 -s public",
    "build:local": "env-cmd -f ./.env-local react-app-rewired build",
    "build:dev": "env-cmd -f ./.env.dev react-app-rewired build",
    "build:prod": "env-cmd -f ./.env.prod react-app-rewired build",
    "build:storybook": "build-storybook -s public",
    "test": "react-app-rewired test",
    "test:serve": "serve -s build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/cli": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.22.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-storybook": "^0.6.7",
    "prop-types": "^15.8.1",
    "react-app-rewired": "^2.2.1",
    "serve": "^14.1.2",
    "typescript": "4.5.2",
    "webpack": "^5.74.0"
  }
